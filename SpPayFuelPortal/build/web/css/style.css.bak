/* BUTTONS */
.btn.outline {
    background: none;
    padding: 1px;
	width:99%;
	margin-bottom:3px;
	outline:none;
}
.btn-primary.outline {
    border: 1px solid #51afd6;
    background-color:#51afd6;
    color:#ffffff;outline:none;
}
.btn-primary.outline:hover, .btn-primary.outline:focus, .btn-primary.outline:active, .btn-primary.outline.active, .open > .dropdown-toggle.btn-primary {
    border: 1px solid #000000;
    background-color:#000000;
    color:#ffffff;outline:none;
}
.btn-primary.outline:active, .btn-primary.outline.active {
    border-color: #007299;
    color: #007299;
    box-shadow: none;outline:none;
}



/* BOX */

.holderbox{display:inline-block;padding:10px;}

.holderinbox{display:block;background-color:#ffffff;border:1px solid #ffffff;-webkit-box-shadow: 0px 0px 13px 0px rgba(50, 50, 50, 0.1);-moz-box-shadow: 0px 0px 13px 0px rgba(50, 50, 50, 0.1);box-shadow: 0px 0px 13px 0px rgba(50, 50, 50, 0.1);overflow:hidden;}
.holderinbox:hover{display:block;background-color:#ffffff;border:1px solid rgba(204,204,204,0.3);-webkit-box-shadow: 0px 0px 13px 0px rgba(50, 50, 50, 0.5);-moz-box-shadow: 0px 0px 13px 0px rgba(50, 50, 50, 0.5);box-shadow: 0px 0px 13px 0px rgba(50, 50, 50, 0.5);overflow:hidden;}

.holderinbox h2{display:block;width:100%;background-color:#ffffff;margin:0px;padding:10px;text-align:left;font-size:22px;color:#aeaeae;font-family:ubuntu;}
.holderinbox:hover h2{display:block;width:100%;background-color:rgba(204,204,204,0.3);margin:0px;padding:10px;text-align:left;font-size:22px;color:#515151;font-family:ubuntu;}
.holderinbox span{display:block;width:100%;background-color:#ffffff;padding:10px;font-family:ubuntu;color:#858585;text-align:center;font-size:12px;}

.holderinbox:hover .submitinput input[type="submit"] {
  width:205px;
  background: #0c780c;
  border: 2px solid #0c780c;
  border-radius: 15px;
  color: white;
  cursor: pointer;
  font-size: 18px;;
  color:#ffffff;
  padding:5px;
  transition: all 0.70s ease;
  outline:none;  
	}


	
/* LOGIN */
img.logologin{display:block;border:0px solid red;margin-left:auto;margin-right:auto;}

.loginbox{display:block;background-color:transparent;border:0px solid red;}

.formlogininput{
	border:0px solid red;
	margin-bottom:10px;  
	font-family:ubuntu;
	font-weight:normal;
}

.textlogininput {
  height: 3em;
  margin: 20px 0;
  position: relative;
  border:0px solid red;
  border-radius:20px;
  padding-left:0px;
  width: 60%;
  margin-left: auto;
  margin-right: auto;
  
}

.textlogininput input,
.textlogininput label {
  cursor: text;
  font-size: 18px;
  font-weight:normal;
  padding: 15px 1% 4px 1%;
  position: absolute;
  transition: all 0.70s ease;
  width:100%;
  margin:0px;
  margin-left:auto;
  margin-right:auto;
  text-align:center;
}

.textlogininput input {
  border:0px;    
  border: 2px solid #afafaf;
  border-radius: 20px;
  outline:none;
  
}

.textlogininput label {
  color: #000000;
  font-weight:normal;
  padding: 0.5em 1%;
}

.textlogininput input.filled ~ label,
.textlogininput input:focus ~ label {
  font-size: 10px;
  position: absolute;
}

.textlogininput input.filled ~ label {
  color: #b1b1b1;
}

.textlogininput input[type="text"].filled,
.textlogininput input[type="password"].filled {
  background: #ffffff;
  color:#000000;
  font-weight:normal;
  border:2px solid #ececec;
}

.textlogininput input[type="text"].filled ~label:after,
.textlogininput input[type="password"].filled ~label:after,{
  color: #b1b1b1;
  content: "✔";
  display: inline-block;
  font-size: 14px;;
  text-rendering: auto;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  position: absolute;
  top: 0.3em;
  right: 0.3em;
  transform: translate(0, 0);
}

.submitlogininput{
	text-align:center;  
	margin-top: 20px;
	border:0px solid red;
	}

		
.submitlogininput input[type="submit"] {
  width:60%;
  background: #0c780c;
  border:0px solid red;
  border-radius: 20px;
  color: white;
  cursor: pointer;
  font-size: 20px;;
  padding:8px;
  transition: all 0.70s ease;
     outline:none;  
	 
 
	}
	

	
.submitlogininput input[type="submit"]:hover {
  width:62%;
  background: #0c780c;
  border:0px solid red;
  border-radius: 20px;
  color: white;
  cursor: pointer;
  font-size: 20px;
  letter-spacing:2px;
  padding:8px;
    outline:none;  
  
}

.submitlogininput input[type="submit"]:active {
  width:62%;
  background: #0c780c;
  border:0px solid red;
  border-radius: 20px;
  color: white;
  cursor: pointer;
  font-size: 20px;;
  padding:8px;
  outline:none;  
}	
	
	
	
/* FORMS */

.forminput{
	border:0px solid red;
	margin-bottom:10px;  
	font-family:ubuntu;
	font-weight:normal;
}

.textinput {
  height: 3em;
  margin: 6px 0;
  position: relative;
  border:0px solid red;
  padding-left:0px;
}

.textinput input,
.textinput label {
  cursor: text;
  font-size: 15px;
  font-weight:normal;
  padding: 15px 1% 4px 1%;
  position: absolute;
  transition: all 0.15s ease;
  width: 100%;
  margin:0px;
}

.textinput input {
  border:0px;
  border-bottom: 2px solid #c9c9c9;
  border-radius: 0px;
  outline:none;
}

.textinput label {
  color: #BCB9B8;
  padding: 0.5em 1%;
}

.textinput input.filled ~ label,
.textinput input:focus ~ label {
  font-size: 10px;
  position: absolute;
}

.textinput input.filled ~ label {
  color: #cccccc;
}

.textinput input[type="text"].filled,
.textinput input[type="email"].filled:valid {
  background: #ffffff;
  border-bottom:2px solid #0c780c;
}

.textinput input[type="text"].filled ~label:after,
.textinput input[type="email"].filled:valid ~label:after {
  color: #0c780c;
  content: "✔";
  display: inline-block;
  font-size: 14px;;
  text-rendering: auto;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  position: absolute;
  top: 0.3em;
  right: 0.3em;
  transform: translate(0, 0);
}

.submitinput{
	text-align:center;  
	margin-top: 20px;
	border:0px solid red;
	}



input[type="submit"] {
  width:200px;
  background: #ffffff;
  border: 2px solid #cccccc;
  border-radius: 15px;
  cursor: pointer;
  font-size: 18px;;
  color:#afafaf;
  padding:5px;
  transition: all 0.15s ease;
  outline:none;  
	}

input[type="submit"]:hover {
  width:210px;
  background: #012d90;
  border: 2px solid #012d90;
  border-radius: 15px;
  cursor: pointer;
  font-size: 18px;;
  color:#ffffff;
  padding:5px;
  outline:none;  
}

input[type="submit"]:active {
  width:210px;
  background: #012d90;
  border: 2px solid #012d90;
  border-radius: 15px;
  color: white;
  cursor: pointer;
  font-size: 18px;;
  color:#ffffff;
  padding:5px;
  outline:none;  
}



